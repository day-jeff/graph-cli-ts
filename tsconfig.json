{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "esModuleInterop": true
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ],
  "typeAcquisition": {
    "include": [
      "@azure/msal-node"
    ]
  },
  "importHelpers": true,
  "noImplicitAny": true,
  "moduleResolution": "nodenext",
  "resolveJsonModule": true,
  "skipLibCheck": true,
  "lib": [
    "esNext"
  ],
  "strictPropertyInitialization": false
}
